<nav class="secondary-menu" role="navigation">
  <ul>
    {% for link in linklists.secondary-menu.links %}
      <li>
        <a class="{% if location == 'side-nav' %}main-menu-link{% else %}secondary-menu-link{% endif %}"
           href="{{ link.url }}">{{ link.title }}</a>
      </li>
    {% endfor %}
  </ul>
</nav>
