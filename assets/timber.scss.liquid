    .main {
        background: #666;

        width: percentage( $mainWidth / $totalWidth );
        height: 100%;
        position: absolute;
        left: 0;

        > div {
            @extend %img-container;
            @extend .clearfix;

            height: 100%;
        }

        .content {
            position: absolute;
            z-index: 1;
            top: percentage( $mainContentTop / $totalHeight );
            left: percentage( $mainLeftMargin / $mainWidth );
        }
    }

    .side {
        background: #999;

        width: percentage(382/1350);
        height: 100%;
        position: absolute;
        right: 0;

