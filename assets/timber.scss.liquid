}



%inline-list-base {
    ul, li {
        margin: 0;
    }

    li {
        display: inline-block;
        vertical-align: top;
    }
}

%nav-menu-base {
    @extend %inline-list-base;

    @include at-query ($min, $postSmall) {
        ul {
            height: 21px;
        }
    }

    .site-footer & {
        @include at-query ($max, $small) {
            ul {
                @include clearfix();
            }

            li {
                display: block;
                width: 50%;
                float: left;
                padding: 0 $gutter/2;
            }
        }
    }
}


