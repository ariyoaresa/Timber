/*================================================================================
  # Layouts
  ================================================================================*/

%wrapper-base {
    @extend .clearfix;

    margin: 0 auto;
}

.wrapper-14-col {
    @extend %wrapper-base;

    max-width: $siteWidth14col;
}

.wrapper {
    @extend %wrapper-base;

    max-width: $siteWidth;
}

.wrapper-10-col {
    @extend %wrapper-base;

    max-width: $siteWidth10Col;
}

.wrapper-8-col {
    @extend %wrapper-base;

    max-width: $siteWidth8Col;
}

%column-base {
    padding-right: $gutter;
    padding-left: $gutter;
}


/* Header */

.site-header {

    /* Using a mixin here since this is shared between media queries */
    @mixin header-column() {
        @include verticalCenterChildElements();
        @include box-sizing( content-box );

        position: relative;
        display: block;
        float: left;

        &:first-child {
            margin-left: 0;
        }
    }

    .layout-theme & {

        .wrapper > div {
            @extend %column-base;
        }

        /* Desktop */
        @include at-query ($min, $postSmall) {

            $headerHeight: 85px;

            padding: $gutter 0;

            height: $headerHeight;

            .wrapper {
                position: relative;
                @extend .clearfix;
            }

            %header-column {
                @include header-column();

                height: $headerHeight - $gridGutter;
            }


            .logo-column {
                @extend %header-column;
            }

            .main-menu-column {
                @extend %header-column;
                width: 432px;
                text-align: right;

                ul {
                    padding-top: 2px;
                }
            }

            .customer-menu-column {
                @extend %header-column;
                text-align: right;
                width: 218px;
            }

            .search-column {
                @extend %header-column;
                width: 220px;
            }

        }
        
        /* Mobile */
        @include at-query ($max, $small) {
            $headerHeight: 44px;

            padding: $gutter/2 0;

            height: $headerHeight;

            .wrapper {
                position: relative;
                @extend .clearfix;
            }

            %header-column {
                @include header-column();

                height: $headerHeight - $gridGutter/2;
            }

            .logo-column {
                @extend %header-column;
            }

            .customer-menu-column {
                @extend  %header-column;
                float: right;
            }

            .main-menu-column,
            .search-column {
                display: none;
            }
        }
    }

    .layout-simplified & {
        padding: $gutter * 2 0 $gutter;

        /* center all child elements */
        > * {
            margin-left: auto;
            margin-right: auto;
        }

        @include at-query ($max, $small) {
            padding: ( $gutter * 1.5 ) 0 ( $gutter * 2 )
        }
    }
}

/* Main Content Layout */

/* This contains all the blocks that are not the header, footer, cart, or slide out side bar. */
.site-content {
    @extend .clearfix;
    display: block;

    .layout-simplified & {
        text-align: center;
        overflow-y: visible;
    }
}

/* Just a wrapper for hooking onto the page content and styling it. */
.page-content {
    @extend .wrapper;
}

/* footer */

.site-footer {

    /* Desktop */
    @include at-query ($min, $postSmall) {
        .first-column {
            @include verticalCenterChildElements();
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        > div {
            display: block;
            padding: 0 $gutter;
        }
    }

    .layout-theme & {
        padding: $gutter * 2 0;

        /* Desktop */
        @include at-query ($min, $postSmall) {

            border-top: 1px solid $borderColor;

            > div {
                @include verticalCenterChildElements();
                float: left;
            }

            .first-column {
                width: 672px;
            }

            .last-column {
                width: 288px;
                text-align: right;
            }
        }

        /* Mobile */
        @include at-query ($max, $small) {
            padding: 0;

            .first-column,
            .last-column {
                border-top: 1px solid $borderColor;
                margin: 0 16px;
                padding: 20px 0;
            }
        }
    }

    .layout-simplified & {
        /* Desktop */
        @include at-query ($min, $postSmall) {
            @include verticalCenterChildElements();

            padding: $gutter*2 0;
            text-align: center;

            .first-column {
                height: 40px;
                border-right: 1px solid $colorLighterGray;
            }

            .last-column {
                padding-left: $gutter * 1.25;
            }
        }
    }
}


