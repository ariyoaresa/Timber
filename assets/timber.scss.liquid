/*============================================================================
  #Pagination
  ==============================================================================*/
.pagination {
    margin-bottom: 1em;
}

/*================ Custom Pagination ================*/
.pagination-custom {
    display: inline-block;
    padding-left: 0;
    margin: 0 0 $gutter;
}

.pagination-custom > li {
    display: inline;
}

.pagination-custom > li > a,
.pagination-custom > li > span {
    position: relative;
    float: left;
    padding: 5px 10px;
    margin-left: -1px;
    line-height: 1.42;
    text-decoration: none;
    background-color: #ffffff;
    border: 1px solid $borderColor;
}

.pagination-custom > li:first-child > a,
.pagination-custom > li:first-child > span {
    margin-left: 0;
    border-bottom-left-radius: 4px;
    border-top-left-radius: 4px;
}

.pagination-custom > li:last-child > a,
.pagination-custom > li:last-child > span {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

.pagination-custom > li > a:hover,
.pagination-custom > li > span:hover,
.pagination-custom > li > a:focus,
.pagination-custom > li > span:focus {
    background-color: #eeeeee;
}

.pagination-custom > .active > a,
.pagination-custom > .active > span,
.pagination-custom > .active > a:hover,
.pagination-custom > .active > span:hover,
.pagination-custom > .active > a:focus,
.pagination-custom > .active > span:focus {
    z-index: 2;
    color: #ffffff;
    cursor: default;
    background-color: $colorBtnPrimary;
    border-color: $colorBtnPrimary;
}

.pagination-custom > .disabled > span,
.pagination-custom > .disabled > a,
.pagination-custom > .disabled > a:hover,
.pagination-custom > .disabled > a:focus {
    color: #999999;
    cursor: not-allowed;
    background-color: $disabledGrey;
}

.pagination-custom-lg > li > a,
.pagination-custom-lg > li > span {
    padding: 10px 16px;
    font-size: 18px;
}

.pagination-custom-lg > li:first-child > a,
.pagination-custom-lg > li:first-child > span {
    border-bottom-left-radius: 6px;
    border-top-left-radius: 6px;
}

.pagination-custom-lg > li:last-child > a,
.pagination-custom-lg > li:last-child > span {
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}

.pagination-custom-sm > li > a,
.pagination-custom-sm > li > span {
    padding: 5px 10px;
    font-size: 0.75em;
}

.pagination-custom-sm > li:first-child > a,
.pagination-custom-sm > li:first-child > span {
    border-bottom-left-radius: 3px;
    border-top-left-radius: 3px;
}

.pagination-custom-sm > li:last-child > a,
.pagination-custom-sm > li:last-child > span {
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}


/*============================================================================
  #Site Header
  ==============================================================================*/

/*================ Dropdowns ================*/
.site-nav--dropdown {
    display: none;
    position: absolute;
    left: 0;
    z-index: 5;

    .supports-no-touch .site-nav--has-dropdown:hover &,
    .site-nav--has-dropdown.nav-hover &,
    .nav-focus + & {
        display: block;
    }

    a {
        background-color: $colorNav;

        &:hover,
        &:active,
        &:focus {
            background-color: darken($colorNav, 10%);
        }
    }
}



/*============================================================================
  #Product Grid and List Views
  ==============================================================================*/
.sold-out {

}

.on-sale {

}

.product-list-image {
    display: block;
    float: left;
    margin-right: $gutter;
    margin-bottom: $gutter;
    max-width: 40%;
}

.product-grid-image {
    display: block;
    margin: 0 auto;

    img {
        display: block;
        margin: 0 auto;
    }
}

/*============================================================================
  #Collection Filters
  ==============================================================================*/
.active-filter {
    font-weight: bold;
}

/*============================================================================
  #Breadcrumbs
  ==============================================================================*/
.breadcrumb {
    margin-bottom: $gutter;

    a,
    span {
        display: inline-block;
        padding: $gutter/2 7px $gutter/2 0;
        margin-right: 7px;

        &:first-child {
            padding-left: 0;
        }
    }
}


/*============================================================================
  #Product Page
  ==============================================================================*/
.product-variants {
    display: none;

    .no-js & {
        display: block;
    }
}

@include at-query($min, $large) {
    .selector-wrapper,
    .product-variants {
        select {
            max-width: 40%;
        }
    }
}

.product-photo-container {
    margin-bottom: $gutter;
}

.product-photo-container,
.product-photo-thumbs {
    a, img {
        display: block;
        margin: 0 auto;
    }

    li {
        margin-bottom: $gutter;
    }
}

/*============================================================================
  #Blogs and Comments
  ==============================================================================*/
.comment {
    margin-bottom: $gutter;

    & + & {
        border-top: 1px solid $borderColor;
        padding-top: $gutter;
    }
}


/*============================================================================
  #Notes and Form Feedback
  ==============================================================================*/
.note,
.errors {
    padding: $gutter/2;
    margin-bottom: $gutter/2;
    border: 1px solid transparent;

    ul,
    ol {
        margin-top: 0;
        margin-bottom: 0;
    }

    li:last-child {
        margin-bottom: 0;
    }

    p {
        margin-bottom: 0;
    }
}

.note {
    border-color: $borderColor;
}

.errors {
    ul {
        list-style: disc outside;
        margin-left: 20px;
    }
}

.form-success {
    color: $successGreen;
    background-color: lighten($successGreen, 65%);
    border-color: $successGreen;
}

.form-error,
.errors {
    color: $errorRed;
    background-color: $errorRedBg;
    border-color: $errorRed;
}


/*============================================================================
  #Cart Page
  ==============================================================================*/
.cart-row {
    margin-top: $gutter;
    padding-top: $gutter;
    border-top: 1px solid $borderColor;

    #ajaxifyDrawer & {
        border-top-color: #444;
    }

    &:first-child {
        margin-top: 0;
    }

    &:first-child {
        padding-top: 0;
        border-top: 0 none;
    }

    a {
        color: $colorTextBody;

        &:hover {
            color: $colorPrimary;
        }
    }
}

.cart-subtotal {
    margin-bottom: $gutter;
}

.cart-subtotal--price {
    margin: 0 0 0 $gutter/3;
    display: inline;
}


/*================================================================================
  # Extends - these are for common text and block styles
  ================================================================================*/

%small-arial-text {
    font-family: $arialFontStack;
    font-weight: 700;

    @include at-query ($min, $postSmall) {
        font-size: 0.8125em;
    }

    @include at-query ($max, $small) {
        font-size: 0.625em;
    }
}    

%smaller-arial-text {
    font-family: $arialFontStack;
    font-weight: 700;

    @include at-query ($min, $postSmall) {
        font-size: 0.6875em;
    }
}

%base-link {
    text-decoration: none;
    white-space: nowrap;
    color: $colorGray;

    &:hover,
    &:active,
    &:focus {
        color: $colorGold;
    }
}

%base-link-inline {
    @extend %base-link;
    @extend %small-arial-text;

    line-height: 1;
}

%base-link-block {
    @extend %base-link;
}

/*================================================================================
  # Layouts
  ================================================================================*/

.wrapper {
    @include clearfix();
    max-width: $siteWidth;
    margin: 0 auto;
    padding: 0 $gutter;

    @include at-query ($min, $small) {
        padding: 0 $gutter;
    }
}


/* Header */

.site-header {
    .layout-theme & {

        padding: $gutter 0;

        $headerHeight: 85px;

        height: $headerHeight;

        .wrapper {
            position: relative;
            @extend .clearfix;
        }

        %header-column {
            @include verticalCenterChildElements();
            position: relative;
            display: block;
            float: left;
            height: $headerHeight - $gridGutter;
            margin-left: $gridGutter;

            &:first-child {
                margin-left: 0;
            }
        }

        .logo-column {
            @extend %header-column;
        }

        .main-menu-column {
            @extend %header-column;
            width: 460px;
            text-align: right;

            ul {
                padding-top: 2px;
            }
        }

        .customer-menu-column {
            @extend %header-column;
            text-align: right;
            width: 218px;
        }
        
        .search-column {
            @extend %header-column;
            width: 220px;
        }

    }

    .layout-simplified & {
        padding: $gutter * 2 0 $gutter;

        /* center child elements */
        > * {
            margin-left: auto;
            margin-right: auto;
        }

        @include at-query ($max, $small) {
            padding: ( $gutter * 1.5 ) 0 ( $gutter * 2 )
        }
    }
}

/* Main Content Layout */

/* This contains all the blocks that are not the header, footer, cart, or slide out side bar. */
.site-content {
    @include clearfix();
    display: block;

    .layout-simplified & {
        text-align: center;
        overflow-y: visible;
    }
}

/* Just a wrapper for hooking onto the page content and styling it. */
.page-content {
    @extend .wrapper;
}

/* footer */

.site-footer {
    @include at-query ($min, $large) {
        padding: $gutter*2 0;
    }

    .layout-simplified & {
        @include at-query ($min, $postSmall) {
            @include verticalCenterChildElements();
            text-align: center;

            .first-column {
                @include verticalCenterChildElements();
                height: 40px;
                border-right: 1px solid $colorLighterGray;
            }

            .last-column {
                padding-left: $gutter * 1.25;
            }
        }

        @include at-query ($max, $small) {
            > div {
                display: block;
            }
        }
    }
}


/*================================================================================
  # Blocks
  ================================================================================*/

.site-logo {
    text-align: center;
    margin: 0;
    height: 40px;
    width: 154px;

    @include at-query ( $max, $small ) {
        height: 22px;
        width: 84px;
    }

    a,
    a:hover,
    a:focus {
        text-decoration: none;
    }

    a, img {
        display: block;
    }

    img {
        margin: 0 auto;
        height: 100%;
        width: 100%;
    }

    /* Fallback .png support for IE8 */
    [src*=".svg"] {
        display: none;
    }

    .supports-svg & {
        [src*=".png"] {
            display: none;
        }

        [src*=".svg"] {
            display: block;
        }
    }
}

/* Heros */

.hero-404 {
    background: #fff url({{ '404-hero-bg.jpg' | asset_url }}) no-repeat center top;
    position: relative;
    border-bottom: 1px solid #f6f6f6;

    .supports-boxshadow & {
        border-bottom: 0;
    }

    .supports-csstransforms.supports-cssanimations & {
        @include prefix('animation', 'fadein 2s ease');
    }

    > div {
        position: relative;
        margin: 0 auto;
        font-weight: 100;
        font-family: $headerFontStack;
        line-height: 1;
        color: #f2f2f2;
        letter-spacing: -8px;

        span {
            opacity: 0.3;
        }

        .supports-csstransforms.supports-cssanimations & {
            @include prefix('animation', 'scaledown 1s ease-out');
        }
    }

    @include at-query ($min, $postSmall) {
        height: 400px;

        .supports-boxshadow & {
            @include prefix('box-shadow', 'inset 0 -10px 9px -6px rgba(0,0,0,0.04)');
        }

        > div {
            top: 100px;
            left: -5px;
            height: 264px;
            width: 582px;
            font-size: 362px;
            overflow: hidden;
        }
    }

    @include at-query ($max, $small) {
        height: 218px;
        background-size: 76px;

        .supports-boxshadow & {
            @include prefix('box-shadow', 'inset 0 -5px 4px -3px rgba(0,0,0,0.04)');
        }

        > div {
            top: 42px;
            left: -5px;
            height: 111px;
            width: 247px;
            font-size: 164px;
        }
    }
}

.main-menu {
    ul, li {
        margin: 0;
    }

    ul {
        height: 21px;
    }

    li {
        display: inline-block;

        @include at-query ($min, $postSmall) {
            border-left: 1px solid $colorLighterGray;

            &:first-child {
                border-left: 0;
            }
        }
    }

    .site-footer & {
        @include at-query ($max, $small) {
            ul {
                @include clearfix(false);
            }
            
            li {
                display: block;
                width: 50%;
                float: left;
                padding: 0 $gutter/2;
            }
        }
    }
}

.main-menu-link {
    @extend %base-link-inline;

    display: block;
    text-transform: uppercase;

    @include at-query ($min, $postSmall) {
        padding: $gutter/4 $gutter * 1.25;
    }

    @include at-query ($max, $small) {
        padding: $gutter/2 0;
    }
}

.customer-menu {
    @include verticalCenterChildElements();

    border-left: 1px solid #f2f2f2;
    padding: 0 $gutter/2;
    height: 41px;

    ul, li {
        height: 21px;
    }        

    ul {
        margin: 0;
    }

    li {
        display: inline-block;
        margin: 0 -3px; /* inline-block spacing fix */
    }
}

.customer-menu-link {
    @extend %base-link-block;
    @extend .clearfix;

    display: block;
    position: relative;

    img, #{$iconSelector} {
        display: block;
        float: left;
        margin-right: $gutter/3;
    }

    #{$iconSelector} {
        height: 1em;
        width: 1em;
    }

    /* used for the avatar */
    img {
        $radius: 11px;

        position: relative;
        top: -2px;
        height: $radius * 2;
        width: $radius * 2;
        @include prefix(border-radius, $radius);
        border: 2px solid $colorWarmGray;
    }

    .label {
        @extend %small-arial-text;
        display: block;
        float: left;
        padding-top: 1px;
        line-height: 15px;

        max-width: 105px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    @include at-query ($min, $postSmall) {
        padding: 3px $gutter/2 2px;
    }

    @include at-query ($max, $small) {

    }
}

.customer-login-link {
/* **todo** add some styles here for the logged in customer avatar. */
}

.customer-profile-link {

}


.cart-count-link {

    #{$iconSelector} {
        position: relative;
    }

    .count {
        @extend %smaller-arial-text;

        display: block;
        position: absolute;
        top: -10px;
        left: 4px;
        background-color: $colorGold;
        color: #fff;
        height: 16px;
        width: 16px;
        padding-top: 1px;
        line-height: 15px;
        @include prefix(border-radius, 2px);
        text-align: center;

        &:before {
            content: '';
            display: block;
            border: 1px solid #fff;
            height: 14px;
            width: 14px;
            position: absolute;
            top: 1px;
            left: 1px;
            @include prefix(border-radius, 1px);
        }
    }

}

.template-404 {
    .page-content {
        padding-top: 16px;
        padding-bottom: 16px;

        p {
            color: $colorLightGray;
        }
    }
}


/*================ Search bar in header ================*/
.search-bar {
    border-bottom: 1px solid $colorLightGray;
    margin-bottom: 0;
    position: relative;

    button, input {
        background: none;
        height: 100%;
        position: absolute;
        display: block;
        bottom: 0;
        left: 0;
    }

    button {
        line-height: 1;
        color: $colorGray;
    }

    input {
        @extend %small-arial-text;
        margin: 0;
        border: none;
        padding: 0;
        width: 100%;

        line-height: 1.2;
        color: $colorWarmLightGray;
    }

    @include at-query ($min, $postSmall) {
        $buttonWidth: 20px;

        width: 220px;
        height: 30px;

        button {
            font-size: 1em;
            width: $buttonWidth;
        }

        input {
            padding-left: $buttonWidth;
        }
    }

    @include at-query ($max, $small) {
        $buttonWidth: 16px;

        height: 27px;
        margin: 0 $gutter/2;

        button {
            font-size: 0.8125em;
            width: $buttonWidth;
        }

        input {
            padding-left: $buttonWidth;
        }
    }
}

/*================================================================================
  # Animations
  ================================================================================*/

@include keyframes-prefix('scaledown'){
    0% {
        opacity: 0;
        @include prefix('transform', 'scale(12)');
    }

    40% {
        opacity: 0;
    }

    100% {
        opacity: 1;
        @include prefix('transform', 'scale(1)');
    }
}

@include keyframes-prefix('fadein'){
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

