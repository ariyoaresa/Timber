.celebrity-gallery {
    background: #000;

    /* these are based off the PSD, not what we are using for the max-width. That's ok, we're only using them for calculated, relative percentage values. */
    $totalWidth: 1350px;
    $totalHeight: 660px;
    $mainWidth: 968px;
    $sideWidth: 382px;
    $mainLeftMargin: 115px;
    $mainContentTop: 272px;
    $thumbHeaderTop: 152px;
    $thumbHeight: 220px;
    $thumbLeftMargin: 16px;

    /* http://wemadeyoulook.at/en/blog/proportional-scaling-responsive-boxes-using-just-css/ */
    position: relative;
    width: 100%;
    height: 0;
    padding-bottom: percentage( $totalHeight / $totalWidth );
    color: #fff;
    background: #000;

    h2 {
        position: absolute;
        z-index: 1;
        top: percentage( 156px / $totalHeight );
        left: percentage( $mainLeftMargin / $mainWidth );
        text-shadow: 0 0 $colorOffWhite;
    }

    /* for slides and the thumb links */
    %img-container {
        position: relative;
    }

    img {
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        left: 0;
        z-index: 0;
    }

    .main {
        width: percentage( $mainWidth / $totalWidth );
        height: 100%;
        position: absolute;
        left: 0;

        > div {
            @extend %img-container;
            @extend .clearfix;

            height: 100%;
        }

    }

    .content {
        position: absolute;
        z-index: 1;
        top: percentage( $mainContentTop / $totalHeight );
        left: percentage( $mainLeftMargin / $mainWidth );

        > * {
            margin: 20px 0;
            line-height: 1;
        }

        h3 {
            font-size: 2.5em;
            font-weight: 300;
            text-shadow: 0 0 $colorOffWhite;
        }

        p {
            font-size: 1.25em;
            font-weight: 100;
            letter-spacing: 0.0625em;
            text-shadow: 0 0 $colorOffWhite;
        }

        a {
            @extend %gold-hover-underline-link;
            @extend %rsaquo;
            @extend %small-arial-text;

            text-transform: uppercase;
        }
    }

    .side {
        width: percentage(382/1350);
        height: 100%;
        position: absolute;
        right: 0;

        > a {
            @extend %img-container;
            @extend .clearfix;

            display: block;
            height: 33.3333%;
            color: #fff;
        }

        h3 {
            position: absolute;
            bottom: percentage( 24px / $thumbHeight );
            left: percentage( $thumbLeftMargin / $sideWidth );
            line-height: 1;
            margin: 0;
            text-shadow: 0 0 $colorOffWhite;            
        }

        img {
            opacity: 0.3;
        }

        .active img {
            opacity: 1;
        }
    }
}

