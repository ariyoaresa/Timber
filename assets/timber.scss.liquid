%hero-base {
    position: relative;
    color: #fff;

    /* White image mask */
    &:before, &:after {
        content: '';
        height: 0px;
        width: 50%;
        display: block;
        position: absolute;
        bottom: 0px;
        border-top: 0;
        border-bottom: 38px solid white;
        z-index: 1000;
    }

    &:before {
        left: 0px;
        border-left: 0;
        border-right: 48px solid transparent;
    }

    &:after {
        right: 0px;
        border-right: 0;
        border-left: 48px solid transparent;
    }
}

