    @extend %gray-hover-gold-link;
    @extend %small-arial-text;

    display: block;
    line-height: 1;
    text-transform: uppercase;

    @include at-query ($min, $postSmall) {
        padding: $gutter/4 $gutter * 1.25;
    }

    @include at-query ($max, $small) {
        padding: $gutter/2 0;
    }
}

.customer-menu {
    @extend %inline-list-base;

    /* Desktop */
    @include at-query ($min, $postSmall) {
        @include verticalCenterChildElements();

        height: 41px;
        padding: 0 $gutter/2;
        border-left: 1px solid #f2f2f2;

        ul, li {
            height: 21px;
        }

        li {
            margin: 0 -2px; /* inline-block spacing fix */
        }

        .side-menu-toggle-block {
            display: none;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {

        &, ul, li {
            height: 22px;
        }

        li {
            margin: 0 0 0 6px; /* inline-block spacing fix, 10px between li with the 2px per side whitespace */
        }

        .profile-login-links-block {
            display: none;
        }
    }
}

.customer-menu-link {
    @extend %gray-hover-gold-link;
    @extend .clearfix;

    display: block;
    position: relative;

    .label {
        @extend %small-arial-text;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        padding: 3px $gutter/2 2px;

        img, #{$iconSelector}, .label {
            display: block;
            float: left;
        }

        img, #{$iconSelector} {
            margin-right: $gutter/3;
        }

        #{$iconSelector} {
            height: 1em;
            width: 1em;
        }

        /* used for the avatar */
        img {
            $radius: 11px;

            position: relative;
            top: -2px;
            height: $radius * 2;
            width: $radius * 2;
            @include prefix(border-radius, $radius);
            border: 2px solid $colorWarmGray;
        }

        .label {
            padding-top: 1px;
            line-height: 15px;
            max-width: 105px;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        height: 22px;
        width: 22px;
        border: 1px solid $colorGray;
        @include prefix(border-radius, 2px);
        text-align: center;

        #{$iconSelector} {
            display: inline-block;
            height: 1em;
            width: 1em;
            font-size: 0.75em;
            padding-top: 4px;
        }

        .label {
            display: none;
        }
    }
}

.cart-count-link {

    #{$iconSelector} {
        position: relative;
    }

    .count {
        @extend %smaller-arial-text;

        display: block;
        position: absolute;
        top: -10px;
        left: 4px;
        background-color: $colorGold;
        color: #fff;
        height: 16px;
        width: 16px;
        padding-top: 1px;
        line-height: 15px;
        @include prefix(border-radius, 2px);
        text-align: center;

        &:before {
            content: '';
            display: block;
            border: 1px solid #fff;
            height: 14px;
            width: 14px;
            position: absolute;
            top: 1px;
            left: 1px;
            @include prefix(border-radius, 1px);
        }
    }
}

.meta-menu {
    @extend %nav-menu-base;
}

.meta-menu-link {
    @extend %gray-hover-underline-link;
    @extend %small-arial-text;
    @extend %rsaquo;

    display: block;
    text-transform: uppercase;
    padding: 4px 12px;

    &:first-child {
        padding-left: 0;
    }
}

.language-menu {
    @extend %nav-menu-base;
}

.language-menu-link {
    @extend %gray-hover-underline-link;
    @extend %small-arial-text;

    display: block;
    text-transform: uppercase;
}


.latest-collections-menu {
    @extend %inline-list-base;

    text-align: center;

    li {
        margin: 0 5px - 2px; /* taking off the 2px for the extra spacing from the inline-block display type. */
        text-align: left;   /* undo the centering for child elements */
    }
}

.latest-collections-link {
    @extend %white-hover-gray-link;
    @extend %small-arial-text;

    $height: 117px;
    $width: 173px;

    text-transform: uppercase;
    display: block;
    position: relative;

    height: $height;
    width: $width;

    /* contains the background image for the transparent border outline */
    &:before {
        content: '';
        position: absolute;
        top: 0;
        bottom: 0;
        width: 100%;

        background-color: #fff;
        background-position: left top;
        background-repeat: no-repeat;
        background-size: $width $height;

        transition: all 0.4s;
    }

    span {
        @extend %rsaquo;

        line-height: 1.15;
        position: absolute;
        top: 46px;
        left: 20px;
        width: 64px;
        white-space: normal;
    }

    img {
        height: $height;
        width: $width;
        position: absolute;
        top: 0;
        left: 0;
        display: block;

        filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'desaturate\'><feColorMatrix type=\'saturate\' values=\'0.43\'/></filter></svg>#desaturate"); /* Firefox 10+, Firefox on Android */
        filter: gray; /* IE6-9 */
        -webkit-filter: grayscale(43%); /* Chrome 19+, Safari 6+, Safari 6+ iOS */

        @include prefix('transition', 'all 0.4s ease');

        clip: rect(5px, $width - 5px, $height - 5px, 5px);
    }

    .overlay {
        @include prefix('box-shadow', 'inset 0px 0px 76px 0px rgba(50, 50, 50, 0.75)');

        background: rgba(black, 0.20);
        position: absolute;
        top: 0;
        bottom: 0;
        width: 100%;
        @include prefix('transition', 'box-shadow 0.4s, opacity 0.4s');
    }

    &:hover {
        img {
            filter: none;
            -webkit-filter: none;
        }

        .overlay {
            opacity: 0;
            @include prefix('box-shadow', 'inset 0px 0px 0px 0px rgba(50, 50, 50, 0.75)');
        }

        &:before {
            opacity: 0.6;
        }
    }
}


/*================ Page Specific Styles ================*/
.template-404 {
    .page-content {
        padding-top: 16px;
        padding-bottom: 16px;

        p {
            color: $colorLightGray;
        }
    }
}


/*================ Search bar in header ================*/
.search-bar {
    border-bottom: 1px solid $colorLightGray;
    margin-bottom: 0;
    position: relative;

    button, input {
        background: none;
        height: 100%;
        position: absolute;
        display: block;
        bottom: 0;
        left: 0;
    }

    button {
        line-height: 1;
        color: $colorGray;
    }

    input {
        @extend %small-arial-text;
        margin: 0;
        border: none;
        padding: 0;
        width: 100%;

        line-height: 1.2;
        color: $colorWarmLightGray;
    }

    @include at-query ($min, $postSmall) {
        $buttonWidth: 20px;

        width: 220px;
        height: 30px;

        button {
            font-size: 1em;
            width: $buttonWidth;
        }

        input {
            padding-left: $buttonWidth;
        }
    }

    @include at-query ($max, $small) {
        $buttonWidth: 16px;

        height: 27px;
        margin: 0 $gutter/2;

        button {
            font-size: 0.8125em;
            width: $buttonWidth;
        }

        input {
            padding-left: $buttonWidth;
        }
    }
}

/*================================================================================
  # Animations
  ================================================================================*/

@include keyframes-prefix('scaledown'){
    0% {
        opacity: 0;
        @include prefix('transform', 'scale(12)');
    }

    40% {
        opacity: 0;
    }

    100% {
        opacity: 1;
        @include prefix('transform', 'scale(1)');
    }
}

@include keyframes-prefix('fadein'){
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}
