    $height: 117px;
    $width: 173px;

    text-transform: uppercase;
    display: block;
    position: relative;

    height: $height;
    width: $width;

    /* contains the background image for the transparent border outline */
    &:before {
        content: '';
        position: absolute;
        top: 0;
        bottom: 0;
        width: 100%;

        background-color: #fff;
        background-position: left top;
        background-repeat: no-repeat;
        background-size: $width $height;

